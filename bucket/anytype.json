{
    "version": "0.32.3",
    "description": "Anytype is a local-first, P2P and open-source note-taking application",
    "homepage": "https://anytype.io/",
    "license": "Proprietary",
    "architecture": {
        "64bit": {
            "url": "https://anytype-release.fra1.cdn.digitaloceanspaces.com/Anytype%20Setup%200.32.3.exe",
            "hash": "cbaf929c16bc93b41ddb3b6b2f50f1fa240c0a67990ccd9839913cd998cb35fb",
            "pre_install": [
                "$7zr = Join-Path $env:TMP '7zr.exe'",
                "Invoke-WebRequest https://www.7-zip.org/a/7zr.exe -OutFile $7zr",
                "Invoke-ExternalCommand $7zr @('x', \"$dir\\$fname\", \"-o$dir\", '-y') | Out-Null",
                "Remove-Item \"$dir\\$fname\", $7zr"
            ]
        }
    },
    "bin": "Anytype.exe",
    "shortcuts": [
        [
            "Anytype.exe",
            "Anytype"
        ]
    ],
    "checkver": {
        "url": "https://download.anytype.io/",
        "regex": "Desktop \\n\\s+?<span class=\"version\">v(\\d+?\\.\\d+?\\.\\d+?)<"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://anytype-release.fra1.cdn.digitaloceanspaces.com/Anytype%20Setup%20$version.exe"
            }
        }
    }
}
